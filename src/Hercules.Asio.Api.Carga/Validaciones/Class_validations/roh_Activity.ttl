	sh:targetClass roh:Activity ;
	sh:property roh:someValuesDataType__roh__Activity__vivo__identifier;
	sh:property roh:someValuesDataType__roh__Activity__roh__title;
	sh:property roh:allValuesDataType__roh__Activity__roh__title;
	sh:property roh:allValuesClass__roh__Activity__vivo__relates;
	sh:property roh:allValuesDataType__roh__Activity__vivo__identifier;
